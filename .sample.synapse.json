{
  "Name": "my-synapse-1",
  "LogConfig": {
    "EnableConsole": true,
    "ConsoleJSONFormat": true,
    "Consolelevel": "debug"
  },
  "Lambdatest": {
    "SecretKey": "add-your-secret-key-here"
  },
  "Git": {
    "Token": "add-your-git-token-here",
    "TokenType": "Bearer"
  },
  "ContainerRegistry": {
    "PullPolicy": "always",
    "Mode": "public"
  },
  "RepoSecrets": {
    "synapse": {
      "SAMPLE_SECRET_KEY": "sample_secret_value"
    }
  }
}